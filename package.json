{
  "name": "rx-dom-vite-seed",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "predev": "npm run clear:cache",
    "prebuild": "npm run clear:cache",
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "clear:cache": "npm run clear:cache:vite && npm run clear:cache:lirx-dom",
    "clear:cache:vite": "node ./tools/clear-cache.js --vite",
    "clear:cache:lirx-dom": "node ./tools/clear-cache.js --lirx-dom",
    "reinstall": "rm -rf ./node_modules && rm -f ./yarn.lock && yarn install"
  },
  "sideEffects": false,
  "devDependencies": {
    "sass": "^1.62.0",
    "terser": "^5.17.1",
    "typescript": "^5.0.4",
    "vite": "^4.3.0"
  },
  "dependencies": {
    "@fluent/syntax": "^0.18.1",
    "@lifaon/jbson": "latest",
    "@lifaon/math": "latest",
    "@lifaon/path": "latest",
    "@lirx/animations": "latest",
    "@lirx/async-task": "latest",
    "@lirx/core": "latest",
    "@lirx/utils": "latest",
    "@lirx/unsubscribe": "latest",
    "@lirx/dom": "latest",
    "@lirx/dom-aot-plugin": "latest",
    "@lirx/dom-material": "latest",
    "@lirx/mdi": "latest",
    "@lirx/router": "latest",
    "@lirx/store": "latest",
    "@uni-fs/core": "latest",
    "messageformat": "4.0.0-1",
    "tslib": "^2.5.0"
  }
}
